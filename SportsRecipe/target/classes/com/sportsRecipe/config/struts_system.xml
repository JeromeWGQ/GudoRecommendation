<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>

	<package name="register" extends="json-default">
		<action name="register" class="com.sportsRecipe.action.UserinfoAction" >
			<result name="success">/index.jsp</result>
			<result name="input">/login.jsp</result>
		</action>
	</package>
	
	<package name="userprofile" extends="json-default">
	<action name="user_update" class="com.sportsRecipe.action.UserProfileAction" method="user_update"></action>
	<action name="user_inf" class="com.sportsRecipe.action.UserProfileAction" method="user_inf"></action>
	</package>
	
	<package name="userimg" extends="json-default">
	<action name="user_img_update" class="com.sportsRecipe.action.UserImgAction" method="user_img_update"></action>
	<action name="user_img_get" class="com.sportsRecipe.action.UserImgAction" method="user_img_get"></action>
	</package>
</struts>